import { inject } from 'vue';
import type { ComicConfig } from '../export.type'

export function useConfig() {
  const COMIC = inject<ComicConfig>('$COMIC')
  if (!COMIC) {
    return {
      SIZE: null,
      THEME: '#e4acc7',
    }
  } else {
    return {
      SIZE: COMIC.size,
      THEME: COMIC.color?.primary
    }
  }

}