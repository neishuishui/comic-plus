.cu-empty {
  padding: 30px 0;
  text-align: center;
  font-size: 14px;
}

.cu-empty__image img {
  width: var(--size, 100%);
  height: var(--size, 100%);
  object-fit: contain;
}

.cu-empty__description {
  margin-top: 20px;
  color: var(--cu-text-color-light);
}

.cu-empty__tools {
  margin-top: 20px;
}
