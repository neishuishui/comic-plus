
import type { ExtractPropTypes } from "vue";

export const checkboxProps = {
  modelValue: {
    type: Boolean,
    default: false
  },
  label: String,
  ispart: {
    type: Boolean,
    default: () => {
      return null;
    }
  },
  disabled: Boolean,
  activeColor: String
} as const

export type CheckboxProps = ExtractPropTypes<typeof checkboxProps>

export const checkboxEmits = {
  ['update:modelValue']: (evt: boolean) => typeof evt === 'boolean',
  change: (evt: boolean) => typeof evt === 'boolean',
}

export type CheckboxEmits = typeof checkboxEmits