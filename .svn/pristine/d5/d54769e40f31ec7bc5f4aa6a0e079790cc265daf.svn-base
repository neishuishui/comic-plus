
import { getCurrentInstance } from 'vue';
export function useConfig() {
  const { proxy }: any = getCurrentInstance();
  if (!proxy.$COMIC) {
    return {
      SIZE: null,
      Z_INDEX: 2000,
      THEME: '#e4acc7'
    }
  }

  return {
    SIZE: proxy.$COMIC.size,
    Z_INDEX: proxy.$COMIC.zIndex || 2000,
    THEME: proxy.$COMIC.theme || '#e4acc7'
  }

}