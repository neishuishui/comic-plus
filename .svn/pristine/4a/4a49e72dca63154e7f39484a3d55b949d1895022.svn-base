<template>
  <component :is="componentName?.value" :size="size"></component>
</template>

<script setup lang="ts">
import { computed, shallowRef } from 'vue';

import Atlas404 from '../svg/404.vue';
import Atlas500 from '../svg/500.vue';
import AtlasNoData from '../svg/no-data.vue';
import AtlasNoRecord from '../svg/no-record.vue';
import AtlasNoPremiss from '../svg/no-premiss.vue';
import AtlasNoCollect from '../svg/no-collect.vue';
import AtlasNoSearch from '../svg/no-search.vue';
import AtlasNoMessage from '../svg/no-message.vue';
import AtlasNoAddress from '../svg/no-address.vue';
import AtlasNoEval from '../svg/no-eval.vue';
import AtlasNoWeb from '../svg/no-web.vue';

import AtlasPageError from '../svg/page-error.vue';

defineOptions({
  name: 'CuAtlas'
});

const props = defineProps({
  svg: String,
  size: {
    type: [String, Number],
    default: 200
  }
});

const atlasEnum = {
  404: shallowRef(Atlas404),
  500: shallowRef(Atlas500),
  ['no-data']: shallowRef(AtlasNoData),
  ['no-record']: shallowRef(AtlasNoRecord),
  ['no-premiss']: shallowRef(AtlasNoPremiss),
  ['no-collect']: shallowRef(AtlasNoCollect),
  ['no-search']: shallowRef(AtlasNoSearch),
  ['no-message']: shallowRef(AtlasNoMessage),
  ['no-web']: shallowRef(AtlasNoWeb),
  ['no-address']: shallowRef(AtlasNoAddress),
  ['no-eval']: shallowRef(AtlasNoEval),
  ['page-error']: shallowRef(AtlasPageError)
};

const componentName = computed(() => {
  return atlasEnum[props.svg];
});
</script>
