@font-face {
  font-family: 'comic-font';
  font-weight: 400;
  src: url('./AlibabaPuHuiTi-2-55-Regular.ttf');
  font-display: swap;
}
[class^='cu-'],
[class*='cu-'] {
  font-family: 'comic-font', '微软雅黑', Arial, 'STXihei';
  box-sizing: border-box;
  vertical-align: middle;
}
[class*=' cu-icon-'],
[class^='cu-icon-'] {
  font-style: normal;
  font-family: 'iconfont' !important;
}
