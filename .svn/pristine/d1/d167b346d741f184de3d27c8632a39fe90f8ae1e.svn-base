<template>
  <div class="cu-elevator-item" :elevator-name="name">
    <slot></slot>
  </div>
</template>

<script setup lang="ts">
import { inject, onBeforeMount } from 'vue';
import '../style/css';
defineOptions({
  name: 'CuElevatorItem'
});
const props = defineProps({
  name: String,
  label: String
});

const setElevatorItem = inject<Function | null>('elevator:setItem', null);

onBeforeMount(() => {
  setElevatorItem && setElevatorItem({ name: props.name, label: props.label });
});
</script>
